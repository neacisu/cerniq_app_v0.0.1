{
  "storage-driver": "overlay2",
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "50m",
    "max-file": "5"
  },
  "live-restore": true,
  "userland-proxy": false,
  "default-ulimits": {
    "nofile": {
      "Name": "nofile",
      "Soft": 65536,
      "Hard": 65536
    }
  },
  "default-address-pools": [
    {
      "base": "172.27.0.0/16",
      "size": 24
    }
  ],
  "metrics-addr": "0.0.0.0:64093"
}
